<% if @partners.size > 0 %>
<div class="sponsors-outer wow fadeInUp">
	<!--Sponsors Carousel-->
	<ul class="sponsors-carousel owl-carousel owl-theme">
		<% @partners.each do |partner| %>
		<li class="slide-item">
			<figure class="image-box"><%= image_tag partner.image.variant(resize_to_limit: [200,200]) %></figure>
		</li>
		<% end %>
	</ul>
</div>
<% end %>
